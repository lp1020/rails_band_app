<h1><%= @song.name %></h1>
<h4>By <%= @band.name %></h4>
<h4> <%= @band.genre %></h4>
<img src=<%= @band.photo%> />
<ul>
  <li><%= @song.album %></li>
  <li><%= @song.release_year %></li>
</ul>
<iframe width="420" height="315"
src="<%= @song.embed_link%>">
</iframe>
 <%= link_to 'Edit this Song!', edit_song_path %>
  <%= link_to 'Delete this Song!', song_path(@song), method: :delete,
  data: { confirm: "Are you sure you want to delete #{@song.name}?" } %>
  <%= link_to 'All songs', songs_path %>
