<%= form_for (@song) do |f| %>
<div class="form-container">
<div class="bandForm">
  <%= f.label :title %>
  <%= f.text_field :name %>
</div>
<div class="bandForm">
  <%= f.label :album %>
  <%= f.text_field :album %>
</div>
<div class="bandForm">
  <%= f.label :release_year %>
  <%= f.text_field :release_year %>
</div>
<div class="bandForm">
  <%= f.label :embed_link %>
  <%= f.text_field :embed_link %>
</div>
<div class="bandButton">
  <%= f.submit %>
  </div>

<% end %>
</div>
