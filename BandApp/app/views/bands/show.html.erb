<div>
  <h3><%= @band.name %></h3>
  <h3><%= @band.country %></h3>
  <h3><%= @band.genre %></h3>
  <img src=<%= @band.photo%> />
</div>


  <% @songs.each do |s|%>
  <p><%= link_to s.name, song_path(s)%></p>
  <br>
  <% end %>

  <%= link_to 'Add a new song!', new_band_song_path(@band) %>
  <%= link_to 'Edit this Band!', edit_band_path %>
  <%= link_to 'Delete this Band!', band_path(@band), method: :delete,
  data: { confirm: "Are you sure you want to delete #{@band.name}?" } %>
  <%= link_to 'All bands', bands_path %>
