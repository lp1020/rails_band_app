<%= form_for (@band) do |f| %>
<div class="form-container">
<div class="bandForm">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div class="bandForm">
  <%= f.label :country %>
  <%= f.text_field :country %>
</div>
<div class="bandForm">
  <%= f.label :genre %>
  <%= f.text_field :genre %>
</div>
<div class="bandForm">
  <%= f.label :photo %>
  <%= f.text_field :photo %>
</div>
<div class="bandButton">
  <%= f.submit %>
  </div>

<% end %>
</div>
