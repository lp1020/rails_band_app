<%= form_for (@band) do |f| %>
<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :country %>
  <%= f.text_field :country %>
</div>
<div>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
</div>
<div>
  <%= f.label :photo %>
  <%= f.text_field :photo %>
</div>
<div>
  <%= f.submit %>
  </div>
<% end %>
